{
    "name": "cline/semver",
    "description": "A full-featured Semantic Versioning 2.0.0 implementation for PHP with parsing, comparison, constraints, and version operations",
    "license": "MIT",
    "keywords": [
        "php",
        "semver",
        "semantic-versioning",
        "version",
        "versioning"
    ],
    "authors": [
        {
            "name": "Brian Faust",
            "email": "brian@cline.sh"
        }
    ],
    "require": {
        "php": "^8.5.0",
        "illuminate/contracts": "^11.0 || ^12.0",
        "illuminate/support": "^11.0 || ^12.0",
        "spatie/laravel-package-tools": "^1.92.7"
    },
    "require-dev": {
        "cline/coding-standard": "^1.6.3",
        "orchestra/testbench": "^8.0 || ^9.0 || ^10.8",
        "symfony/var-dumper": "^6.0 || ^7.4"
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Cline\\SemVer\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "ergebnis/composer-normalize": true,
            "pestphp/pest-plugin": true
        }
    },
    "extra": {
        "laravel": {
            "aliases": {
                "SemVer": "Cline\\SemVer\\Facades\\SemVer"
            },
            "providers": [
                "Cline\\SemVer\\SemVerServiceProvider"
            ]
        }
    },
    "scripts": {
        "lint": "vendor/bin/ecs check --fix",
        "refactor": "rector",
        "test": [
            "@test:lint",
            "@test:type-coverage",
            "@test:unit",
            "@test:types",
            "@test:refactor"
        ],
        "test:lint": "vendor/bin/ecs check",
        "test:refactor": "rector --dry-run",
        "test:type-coverage": "pest --parallel --min=80 --type-coverage",
        "test:types": "phpstan --memory-limit=4G",
        "test:unit": "pest --parallel --min=80 --coverage"
    }
}
